    "object FizzBuzz extends App {\n",
    "\n",
    "  for ( j <- 0 to 100)  {\n",
    "    if ( j % 3 == 0)\n",
    "      println(\"Fizz\")\n",
    "    else if ( j % 5 == 0)\n",
    "      println(\"Buzz\")\n",
    "    else if ( j%3 == 0 && j % 5 == 0 )\n",
    "      println(\"FizzBuzz\")\n",
    "    else\n",
    "      println(j)\n",
    "  }\n",
    "\n",
    "\n",
    "}\n",
    "\n",
    "FizzBuzz.main(Array(\"1\",\"2\"))"
